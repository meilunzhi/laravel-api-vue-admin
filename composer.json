{
    "name": "meilunzhi/laravel-vue-admin",
    "description": "laravel-vue-admin",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "meilunzhi",
            "email": "meilunzhi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0|^8.1",
        "league/fractal": "^0.20.1",
        "tymon/jwt-auth": "^2.0",
        "laravel/framework": "^9.52.7",
        "spatie/laravel-permission": "^5.10.1"
    },
    "autoload": {
        "psr-4": {
            "Meilunzhi\\Admin\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true
        },
        "audit": {
            "ignore": [
                "PKSA-8qx3-n5y5-vvnd",
                "PKSA-w7xr-vk7n-rstm"
            ]
        },
        "block-insecure": false
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meilunzhi\\Admin\\AdminServiceProvider",
                "Meilunzhi\\Admin\\UEditorProvider"
            ],
            "aliases": {
                "Service": "Meilunzhi\\Admin\\"
            }
        }
    }
}
